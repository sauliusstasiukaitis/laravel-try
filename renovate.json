{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base"
	],
	"packageRules": [{
		"matchDepTypes": ["devDependencies"],
		"groupName": "devDependencies",
		"automerge": true,
		"prPriority": -1
	}, {
		"matchUpdateTypes": ["patch", "minor"],
		"groupName": "dependencies (non-major)"
	}, {
		"prhourlylimit": 10
	}]
}
